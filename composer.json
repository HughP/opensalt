{
    "name": "pcgus/cftool",
    "license": "proprietary",
    "copyright": "Copyright 2016 by Public Consulting Group",
    "type": "project",
    "description": "Standards Alignment Tool",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.1",
        "fxp/composer-asset-plugin": "^1.2",

        "symfony/symfony": "3.1.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",

        "twig/extensions": "^1.3",

        "symfony/assetic-bundle": "^2.8",

        "symfony/polyfill-iconv": "^1.0",
        "jms/di-extra-bundle": "^1.7",
        "jms/serializer-bundle": "^1.1",

        "doctrine/doctrine-migrations-bundle": "^1.1",
        "stof/doctrine-extensions-bundle": "^1.2",
        "doctrine/doctrine-fixtures-bundle": "^2.3",

        "ramsey/uuid": "^3.5",
        "ramsey/uuid-doctrine": "^1.2",

        "csa/guzzle-bundle": "^2.1",

        "nelmio/security-bundle": "^2.0",

	"nilportugues/jsonapi-bundle": "^1.5.2",

        "phpoffice/phpexcel": "^1.8.1",

	"nelmio/api-doc-bundle": "^2.13",

        "bower-asset/jquery": "^2.2.4",
        "bower-asset/jquery-ui": "^1.12.0",
        "bower-asset/fancytree": "^2.19.0",
        "bower-asset/ui-contextmenu": "^1.12.0",
        "bower-asset/bootstrap-sass": "^3.3.6",
        "bower-asset/html5-boilerplate": "^5.3.0",
        "bower-asset/modernizr": "^3.3.1",
        "bower-asset/normalize.css": "^3.0.3",
        "npm-asset/jquery-contextmenu": "^2.2.4",
	"bower-asset/bootstrap-multiselect": "^0.9.13",

        "bower-asset/select2": "^4.0.3",
        "bower-asset/select2-bootstrap-theme": "dev-master",
	"tetranz/select2entity-bundle": "2.*",

        "npm-asset/iso639-js": "^1.1",

	"fortawesome/font-awesome": "^4.6",
	"mervick/material-design-icons": "^2.2",


        "ocramius/proxy-manager": "^1.0.2"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",

        "behat/behat": "^3.1",
        "behat/symfony2-extension": "^2.1",
        "emuse/behat-html-formatter": "^0.1.0",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-browserkit-driver": "^1.3",

        "rmiller/behat-spec": "^0.3.0",

        "phpspec/phpspec": "^2.5",
        "phpspec/nyan-formatters": "^1.0",
        "ciaranmcnulty/phpspec-typehintedmethods": "^1.1.3",

        "squizlabs/php_codesniffer": "^2.6",
        "escapestudios/symfony2-coding-standard": "^2.9",
        "friendsofphp/php-cs-fixer": "^1.11"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "0.1-alpha"
        }
    }
}
