{% extends 'base.html.twig' %}

{% block body %}
{#<img src="{{ asset('assets/img/association-icon.png') }}" width="300px" height="150px"> #}

 {% if ext == 'mp3' %}
    <audio controls>
      <source src="data:audio/wav;base64,{{ contents}}" type="audio/mpeg">
      Your browser does not support the audio tag.
    </audio>
 {% else %}
     
    <video width="320" height="240" controls>
      <source src="data:video/webm;base64,{{contents}}"  type="video/mp4">
      Your browser does not support the video tag.
    </video>
      
  {% endif %} 
  
{% endblock %}
